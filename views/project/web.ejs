            <a class="nav-link dropdown-toggle" href="#" id="nav-btn" data-bs-toggle="dropdown">
              <i class="fas fa-terminal"></i> Projeler</a>
            <ul class="dropdown-menu" aria-labelledby="navbarDropdown">
              <li><a class="dropdown-item" href="/project/mobile"><i class="fas fa-mobile-alt"></i> Mobil</a></li>
              <li><a class="dropdown-item" href="/project/web"><i class="far fa-window-maximize"></i> Web</a></li>
            </ul>
          </li>
          <li class="navbar-item">
            <a class="nav-link active" id="nav-btn" href="/aboutus"><i class="fas fa-address-card"></i> Hakkımızda</a>
          </li>
          <li class="navbar-item">
            <a class="nav-link active" id="nav-btn" href="/aboutus#contact"><i class="fas fa-headset"></i> İletişim</a>
          </li>
          <li class="navbar-item">
            <a class="nav-link active" id="nav-btn" href="/blog"><i class="fas fa-blog"></i> Blog</a>
          </li>
        </ul>
      </div>
      </div>
    </nav>

    <div style="padding-top: 250px; padding-bottom: 260px;" class="text-center text-light">
      <div class="container">
      <h1>Web Projeler</h1>
      <p class="pb-5">Müşterilerin isteği üzerine yaptığımız web yazılımlar.</p>
    </div>
  </section>


  <section class="section pt-5 pb-5">
    <h1 align="center">Projeler</h1>
    <div class="container">
      <div class="row">
    <% for(i=0;i < web.length; i++){ %>
        <div class="col-3">
          <a href="<%= web[i].link %>"><div>
                <img src="<%= web[i].icon %>" width="150px" height="150px" alt="project">
              </div></a>
        </div>
    <% } %>
  </div></div>
  </section>



